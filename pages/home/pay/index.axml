<!--2.商品view-->
<view class="goodsView">
<view class="leftView">
<image src="../../../image/buy.png"></image>
</view>
<view class="rightView">
<view class="titleView">
<text>限量版护手霜</text>
</view>
<!--<view class="guigeView">
