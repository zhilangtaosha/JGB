<!--3.邮费view-->
<view class="postageView">
<text>邮费</text>
<text class="postageText">商家包邮</text>
</view>

<!--3.运费险view-->
<view class="freightView">
<text>运费险</text>
<text class="freightText">商家赠送</text>
</view>


<!--3.抢购成功view-->
<view class="sucView">
<text>抢购成功</text>
<text class="sucSubsidyText">补贴30元</text>
</view>


<!--4.抢购失败view-->
<view class="faiView">
<text>抢购失败</text>
<text class="faiSubsidyText">补贴12元</text>
</view>

<!--5.商品价view-->
<view class="goodsPriceView">
<text>商品价</text>
<text class="oldPriceText">¥200</text>
<text class="newPriceText">¥120</text>
</view>


<!--6.总价view-->
<view class="msgView">
<text>留言</text>
<input placeholder="给商家留言"  maxlength="50"/>
</view>


<!--7.提交订单view-->
<view class="orderView">
<view class="hejiView">合计：¥120</view>
<view class="commitView" catchTap="commitOrder">提交订单</view>
</view>

</view>

