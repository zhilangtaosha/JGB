<view class="colorView">
<text>颜色</text>
<text class="colorText">白色</text>
</view>
<view class="sizeView">
<text>大小</text>
<text class="sizeText">M码</text>
</view>
<view class="countView">
<text>数量</text>
<text class="countText">x 1</text>
</view>
</view>
